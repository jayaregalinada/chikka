{
    "name": "jag/chikka",
    "type": "library",
    "description": "Chikka and Laravel 5",
    "keywords": [
        "jag",
        "chikka",
        "jayaregalinada",
        "philippines"
    ],
    "homepage": "https://github.com/jag/chikka",
    "license": "MIT",
    "authors": [
        {
            "name": "Jay Are Galinada",
            "email": "jayaregalinada@gmail.com",
            "homepage": "jayaregalinada.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0",
        "illuminate/support": ">=5.1",
        "illuminate/database": ">=5.1",
        "doctrine/dbal": "^2.5",
        "predis/predis": "~1.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Jag\\Chikka\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jag\\Chikka\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}

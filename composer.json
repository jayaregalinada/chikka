{
    "name": "jag/chikka",
    "type": "library",
    "description": "Chikka and Laravel 5",
    "keywords": [
        "jag",
        "chikka",
        "jayaregalinada",
        "philippines",
        "sms",
        "messaging"
    ],
    "homepage": "https://github.com/jayaregalinada/chikka",
    "license": "MIT",
    "authors": [
        {
            "name": "Jay Are Galinada",
            "email": "jayaregalinada@gmail.com",
            "homepage": "http://jayaregalinada.github.io/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.6.4",
        "illuminate/support": "~5.1",
        "guzzlehttp/guzzle": "~6"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Jag\\Chikka\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        },
        "laravel": {
            "providers": [
                "Jag\\Chikka\\ServiceProvider"
            ],
            "aliases": {
                "Chikka": "Jag\\Chikka\\ChikkaFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
